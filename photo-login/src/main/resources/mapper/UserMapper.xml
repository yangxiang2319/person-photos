<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yx.photo.login.mapper.UserMapper" >

    <select id="getUser" resultType="com.yx.photo.user.User">
        SELECT
            id,user_name,password,user_state,role_type,create_time,update_time
        FROM
            user
        WHERE
            user_name = #{userName}
    </select>

</mapper>